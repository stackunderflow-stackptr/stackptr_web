{
    "version": 2,
    "controller": {},
    "workers": [
        {
            "type": "router",
            "options": {
                "pythonpath": [
                    ".."
                ]
            },
            "realms": [
                {
                    "name": "stackptr",
                    "roles": [
                        {
                            "name": "authenticator",
                            "permissions": [
                                {
                                    "uri": "com.stackptr.authenticate",
                                    "match": "exact",
                                    "allow": {
                                        "call": false,
                                        "register": true,
                                        "publish": false,
                                        "subscribe": false
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": false
                                }
                            ]
                        },
                        {
                            "name": "api",
                            "permissions": [
                                {
                                    "uri": "com.stackptr.api.",
                                    "match": "prefix",
                                    "allow": {
                                        "call": false,
                                        "register": true,
                                        "publish": false,
                                        "subscribe": false
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": false
                                },
                                {
                                    "uri": "com.stackptr.user",
                                    "match": "exact",
                                    "allow": {
                                        "call": false,
                                        "register": false,
                                        "publish": true,
                                        "subscribe": false
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": false
                                },
                                {
                                    "uri": "com.stackptr.group",
                                    "match": "exact",
                                    "allow": {
                                        "call": false,
                                        "register": false,
                                        "publish": true,
                                        "subscribe": false
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": false
                                }
                            ]
                        },
                        {
                            "name": "sessionmonitor",
                            "permissions": [
                                {
                                    "uri": "wamp.session.",
                                    "match": "prefix",
                                    "allow": {
                                        "call": false,
                                        "register": false,
                                        "publish": false,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": false
                                }
                            ]
                        },
                        {
                            "name": "anonymous",
                            "permissions": [
                                {
                                    "uri": "",
                                    "match": "prefix",
                                    "allow": {
                                        "call": false,
                                        "register": false,
                                        "publish": false,
                                        "subscribe": false
                                    },
                                    "disclose": {
                                        "caller": false,
                                        "publisher": false
                                    },
                                    "cache": false
                                }
                            ]
                        },
                        {
                            "name": "user",
                            "permissions": [
                                {
                                    "uri": "com.stackptr.user",
                                    "match": "exact",
                                    "allow": {
                                        "call": false,
                                        "register": false,
                                        "publish": false,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": true,
                                        "publisher": false
                                    },
                                    "cache": false
                                },
                                {
                                    "uri": "com.stackptr.group",
                                    "match": "exact",
                                    "allow": {
                                        "call": false,
                                        "register": false,
                                        "publish": false,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": true,
                                        "publisher": false
                                    },
                                    "cache": false
                                },
                                {
                                    "uri": "com.stackptr.alert",
                                    "match": "exact",
                                    "allow": {
                                        "call": false,
                                        "register": false,
                                        "publish": false,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": true,
                                        "publisher": false
                                    },
                                    "cache": false
                                },
                                {
                                    "uri": "com.stackptr.api.",
                                    "match": "prefix",
                                    "allow": {
                                        "call": true,
                                        "register": false,
                                        "publish": false,
                                        "subscribe": false
                                    },
                                    "disclose": {
                                        "caller": true,
                                        "publisher": false
                                    },
                                    "cache": false
                                }
                            ]
                        }
                    ]
                }
            ],
            "transports": [
                {
                    "type": "web",
                    "endpoint": {
                        "type": "tcp",
                        "interface": "127.0.0.1",
                        "port": 9001
                    },
                    "paths": {
                        "/": {
                            "type": "static",
                            "directory": "/var/empty"
                        },
                        "ws": {
                            "type": "websocket",
                            "auth": {
                                "ticket": {
                                    "type": "dynamic",
                                    "authenticator": "com.stackptr.authenticate"
                                }
                            }
                        }
                    },
                    "options": {
                        "access_log": true,
                        "debug": true
                    }
                },
                {
                    "type": "web",
                    "endpoint": {
                        "type": "tcp",
                        "interface": "127.0.0.1",
                        "port": 9000
                    },
                    "paths": {
                        "/": {
                            "type": "publisher",
                            "realm": "stackptr",
                            "role": "stackptr",
                            "options": {
                                "require_ip": [
                                    "127.0.0.1"
                                ]
                            }
                        }
                    }
                }
            ],
            "components": [
                {
                    "type": "class",
                    "classname": "stackptr-realtime.StackPtrAuthenticator",
                    "realm": "stackptr",
                    "role": "authenticator"
                },
                {
                    "type": "class",
                    "classname": "stackptr-realtime.StackPtrAPI",
                    "realm": "stackptr",
                    "role": "api"
                },
                {
                    "type": "class",
                    "classname": "stackptr-realtime.StackPtrSessionMonitor",
                    "realm": "stackptr",
                    "role": "sessionmonitor"
                }
            ]
        }
    ]
}
